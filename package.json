    "typescript": "^4.9.3",
    "vite": "^4.0.0"
  },
  "eslintConfig": {
    "extends": "./node_modules/standard/eslintrc.json",
    "parser": "@typescript-eslint/parser",
    "rules": {
      "react/jsx-filename-extension": [
